#include <bits/stdc++.h>

using namespace std;



int main()
{
    double total = 0;
    string s = "";
    double num= 0;
    cin >> total;
    while (true) {
        cin >> s;
        if (s == "+") {
            cin >> num;
            total = total + num;
            continue;
        }

        if (s == "-") {
            cin >> num;
            total = total - num;
            continue;
        }

        if (s == "*") {
            cin >> num;
            total = total * num;
            continue;
        }
        if (s == "/") {
            cin >> num;
            total = total / num;
            continue;
        }

        if (s == "exit") {
            break;
        }

      
    }

    cout << total; 
}
